{
    "name": "mintermania/centrifuge-broadcaster",
    "type": "library",
    "description": "Centrifuge broadcaster for laravel >= 5.6",
    "keywords": ["laravel", "centrifuge", "centrifugo", "broadcaster", "websocket"],
    "license": "MIT",
    "authors": [
        {
            "name": "Emprove",
            "email": "skyflap@gmail.com"
        },
        {
            "name": "Nikita Stenin",
            "email": "stenin.nikita@gmail.com"
        },
        {
            "name": "LuisOzuna",
            "email": "lgozuna@gmail.com"
        }
    ],
    "homepage": "https://github.com/emprove/centrifuge-broadcaster",
    "support": {
        "issues": "https://github.com/emprove/centrifuge-broadcaster/issues",
        "source": "https://github.com/emprove/centrifuge-broadcaster"
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3.3",
        "predis/predis": "^1.1.1",
        "illuminate/broadcasting": "^6.0.4",
        "lcobucci/jwt": "3.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0.1",
        "phpunit/phpunit": "^8.3.5"
    },
    "autoload": {
        "psr-4": {
            "LaraComponents\\Centrifuge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraComponents\\Centrifuge\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraComponents\\Centrifuge\\CentrifugeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
