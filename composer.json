{
    "name": "emprove/centrifuge-broadcaster",
    "type": "library",
    "description": "Centrifuge broadcaster for laravel >= 5.7",
    "keywords": ["laravel", "centrifuge", "centrifugo", "broadcaster"],
    "license": "MIT",
    "authors": [
        {
            "name": "Emprove",
            "email": "skyflap@gmail.com"
        }
    ],
    "homepage": "https://github.com/emprove/centrifuge-broadcaster",
    "support": {
        "issues": "https://github.com/emprove/centrifuge-broadcaster/issues",
        "source": "https://github.com/emprove/centrifuge-broadcaster"
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/broadcasting": "5.7.*",
        "guzzlehttp/guzzle": "^6.3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^3.3",
        "phpunit/phpunit": "^5.7|^6.1"
    },
    "autoload": {
        "psr-4": {
            "LaraComponents\\Centrifuge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraComponents\\Centrifuge\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraComponents\\Centrifuge\\CentrifugeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}